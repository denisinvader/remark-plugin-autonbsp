{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "skipLibCheck": false,
        "moduleResolution": "bundler",
        "isolatedModules": true,
        "moduleDetection": "force",
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "noUncheckedSideEffectImports": true
    }
}
